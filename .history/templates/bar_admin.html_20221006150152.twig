{% block barnav %}
	<div align='center'>
		<div class="row" style="margin:auto !important; ">
			<div class="col-12 col-md-12 col-lg-12">
				<table class="table navBarBackGround">

					<tr class='text-center'>
						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["usuarios"]["titulo"] }}'>
								<button class="btn btn-outline-primary btn-lg" title='{{ iconos["usuarios"]["titulo"] }}' onclick="document.location='{{ iconos["usuarios"]["url"] }}'"><img src='{{ iconos["usuarios"]["icon"] }}' class="icon_nav_bar"></button>
						</td>
						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["documentos"]["titulo"] }}'>
							<a href='{{ iconos["documentos"]["url"] }}' style="text-decoration:none !important">
								<img src='{{ iconos["documentos"]["icon"] }}' class="icon_nav_bar">
							</a>
						</td>
						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["auditoria"]["titulo"] }}'>
							<a href='{{ iconos["auditoria"]["url"] }}' style="text-decoration:none !important">
								<img src='{{ iconos["auditoria"]["icon"] }}' class="icon_nav_bar">
							</a>
						</td>
						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["mejoramiento"]["titulo"] }}'>
							<a href='{{ iconos["mejoramiento"]["url"] }}' style="text-decoration:none !important">
								<img src='{{ iconos["mejoramiento"]["icon"] }}' class="icon_nav_bar">
							</a>
						</td>

						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["organizacion"]["titulo"] }}'>
							<a href='{{ iconos["organizacion"]["url"] }}' style="text-decoration:none !important">
								<img src='{{ iconos["organizacion"]["icon"] }}' class="icon_nav_bar">
							</a>
						</td>
						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["indicadores"]["titulo"] }}'>
							<a href='{{ iconos["indicadores"]["url"] }}' style="text-decoration:none !important" target="_blank">
								<img src='{{ iconos["indicadores"]["icon"] }}' class="icon_nav_bar">
							</a>
						</td>
						<td onmouseover="this.style.background='#f5f5f5';" onmouseout="this.style.background='#cbdbfc';" title='{{ iconos["salida"]["titulo"] }}'>
							<a href='{{ iconos["salida"]["url"] }}' style="text-decoration:none !important">
								<img src='{{ iconos["salida"]["icon"] }}' class="icon_nav_bar">
							</a>
						</td>
					</tr>
				</table>
			</div>
		</div>
	</div>
{% endblock %}
